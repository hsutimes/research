<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.hsu.research.dao.BftjDao">
    <select id="queryBftj" resultType="org.hsu.research.entity.Bftj">
        select bftjdm,bftj
        from bftj;
    </select>
    <select id="queryBftjByDm" resultType="org.hsu.research.entity.Bftj">
        select bftjdm,bftj
        from bftj
        where bftjdm = #{bftjdm};
    </select>
</mapper>